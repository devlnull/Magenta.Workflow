@model IEnumerable<FlowType>

<div class="row">
    <div class="col-md-12">
        <h2>FlowTypes</h2>
        <div class="row">
            <div class="col-md-12">
                <a asp-action="Create" class="btn btn-primary">Create a FlowType</a>
            </div>
        </div>
        <br />

        <table class="table table-striped">
            <thead>
                <tr>
                    <th scope="col">Name</th>
                    <th scope="col">CreatedAt</th>
                    <th scope="col">ModifiedAt</th>
                    <th scope="col">Deleted</th>
                    <th scope="col">EntityType</th>
                    <th scope="col">EntityPayloadType</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.Name</td>
                        <td>@item.CreatedAt</td>
                        <td>@item.ModifiedAt</td>
                        <td>@item.Deleted</td>
                        <td>@item.EntityType</td>
                        <td>@item.EntityPayloadType</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>